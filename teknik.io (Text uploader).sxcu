{
  "Name": "teknik.io (Text uploader)",
  "RequestURL": "https://api.teknik.io/v1/Paste",
  "Arguments": {
    "code": "$input$",
    "title": "",
    "syntax": "auto-detect",
    "expireUnit": "never",
    "password": "",
    "hide": "true",
    "doNotTrack": "true"
  },
  "Headers": {
    "Authorization": "Basic $base64:YOUR_USERNAME_HERE:YOUR_TOKEN_HERE$"
  },
  "URL": "$json:result.url$"
}