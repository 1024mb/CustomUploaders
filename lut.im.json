{
  "Name": "lut.im",
  "RequestType": "POST",
  "RequestURL": "https://lut.im/",
  "FileFormName": "file",
  "Arguments": {
    "format": "json",
    "delete-day": "0",
    "first-view": "0"
  },
  "ResponseType": "Text",
  "RegexList": [
    "\"short\":\"(?<short>\\w+\\/\\w+)\"",
    "\"real_short\":\"(?<real_short>\\w+)\"",
    "\"token\":\"(?<token>\\w+)\""
  ],
  "URL": "https://lut.im/$1,short$",
  "ThumbnailURL": "",
  "DeletionURL": "https://lut.im/d/$2,real_short$/$3,token$"
}