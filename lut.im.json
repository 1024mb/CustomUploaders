{
  "Name": "lut.im",
  "RequestType": "POST",
  "RequestURL": "https://lut.im/",
  "FileFormName": "file",
  "Arguments": {
    "format": "json",
    "delete-day": "0",
    "first-view": "0",
    "crypt": "0",
    "keep-exif": "0"
  },
  "ResponseType": "Text",
  "RegexList": [
    "\"short\": *\"(?<short1>\\w+?)\\\\/(?<short2>\\w+)\"",
    "\"real_short\": *\"(?<real_short>\\w+)\"",
    "\"token\": *\"(?<token>\\w+)\""
  ],
  "URL": "https://lut.im/$1,short1$/$1,short2$",
  "ThumbnailURL": "",
  "DeletionURL": "https://lut.im/d/$2,real_short$/$3,token$"
}