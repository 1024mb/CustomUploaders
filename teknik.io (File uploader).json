{
  "Name": "teknik.io (File uploader)",
  "RequestType": "POST",
  "RequestURL": "https://api.teknik.io/upload/post",
  "FileFormName": "file",
  "Arguments": {
    "get_delete_key": "yes"
  },
  "ResponseType": "Text",
  "URL": "$json:[0].results.file.url$",
  "DeletionURL": "$json:[0].results.file.url$/$json:[0].results.file.delete_key$"
}